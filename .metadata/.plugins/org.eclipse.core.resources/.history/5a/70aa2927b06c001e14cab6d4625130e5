package com.jdc.weekend.entity;

import jakarta.persistence.EmbeddedId;
import jakarta.persistence.MapsId;
import lombok.Data;

@Data
public class ProductSale {

	@EmbeddedId
	private ProductSalePk id;
	
	@MapsId("prductId")
	private int productId;
	@MapsId("saleId")
	private int saleId;
	private int quantity;
	
}
